{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm db:migrate && pnpm build",
  "outputDirectory": "dist/sd-plandi/browser",
  "devCommand": "pnpm start",
  "installCommand": "pnpm install",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
