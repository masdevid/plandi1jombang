# Production Environment Variables for Docker Deployment
# Copy this file to .env.production and update with your actual values

# Database Configuration (for Docker/Dedicated Server)
DB_NAME=sd_plandi
DB_USER=sd_plandi_user
DB_PASSWORD=CHANGE_THIS_TO_SECURE_PASSWORD

# Application Configuration
NODE_ENV=production
PORT=3000
APP_PORT=3000

# Security (optional - for future enhancements)
# JWT_SECRET=your-jwt-secret-here
# SESSION_SECRET=your-session-secret-here

# Nginx SSL (if using nginx profile)
# DOMAIN=your-domain.com
